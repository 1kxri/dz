#define _USE_MATH_DEFINES 
#define _CRT_SECURE_NO_WARNINGS
#define ROW 9
#define COL 9 
#define STOLB 4
#define STROK 5
#include <stdio.h>
#include <math.h>
#include <locale.h>
#include <conio.h>
void main() {
	int row, col;
	printf("\n");
	for (row = 1; row <= ROW; ++row) {
		for (col = 1; col <= COL; col++)
			printf("%5d", col * row);
		printf("\n");
	}
	int stolb, strok;
	printf("\n");
	for (stolb = 0; stolb <= STOLB; stolb += 1) {
		for (strok = 0; strok <= STROK; strok += 1)
			printf("%5d", stolb);
		printf("\n");
	}
	system("pause");
}
